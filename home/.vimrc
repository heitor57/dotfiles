" Requirements: YouCompleteMe folder and run install.py
" Example of folder: ~/.vim/plugged/YouCompleteMe
" ./install.py

set tabstop=4
set shiftwidth=4
set encoding=utf-8
set background=dark
set nu
set hlsearch
set incsearch

set foldmethod=indent
set foldlevel=99

syntax on


"if has('gui_running')
"  set background=dark
"  colorscheme solarized
"else
"  colorscheme zenburn
"endif
