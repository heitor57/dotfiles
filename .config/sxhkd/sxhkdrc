XF86MonBrightnessDown
    xbacklight -dec 10
XF86MonBrightnessUp
    xbacklight -inc 10
super + Return
    cd ~ && $TERMINAL
super + F4
    sxiv -t ~/Pictures
super + shift + i
    slock
XF86AudioRaiseVolume
    i3-volume -e 50 -n -N xosd up 5
# pactl set-sink-volume @DEFAULT_SINK@ +5%
XF86AudioLowerVolume
    i3-volume -e 50 -n -N xosd down 5
# pactl set-sink-volume @DEFAULT_SINK@ -5%
XF86AudioMute
    i3-volume -e 50 -n -N xosd mute
# pactl set-sink-mute @DEFAULT_SINK@ toggle
super + F11
    exec xbacklight -dec 10
super + F12
    exec xbacklight -inc 10
super + shift + a
    rifle "$(find ~/Documents/ ~/Downloads/ | dmenu -i -l 10 -fn Monospace-11)"
super + e
    emacsclient -c
super + c
    firefox
super + z
    zathura
super + g
    $TERMINAL -e ranger ~
super + x
    import /tmp/latest-screenshot.png && xclip -selection clipboard -t image/png /tmp/latest-screenshot.png
