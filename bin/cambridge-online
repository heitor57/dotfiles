#!/usr/bin/bash

if [ $XDG_SESSION_TYPE = wayland ]
then
	gtk-launch $(xdg-settings get default-web-browser) "https://dictionary.cambridge.org/dictionary/english/$(wl-paste --primary)"
	echo "eowqejwq"
else
	gtk-launch $(xdg-settings get default-web-browser) "https://dictionary.cambridge.org/dictionary/english/$(xsel -o)"
fi
