#!/usr/bin/bash

if [ "$*" != "" ]
then
	temp="$*"
elif [ $XDG_SESSION_TYPE = wayland ]
then
	temp=$(wl-paste --primary)
else
	temp=$(xsel -o)
fi
# xdotool key --delay=0 Super_L+2
xdotool set_desktop 1
sleep 0.1
xdotool mousemove 900 950
sleep 0.1
xdotool click 1
sleep 0.1
xdotool type --delay 10 "$temp"
sleep 0.3
xdotool key Enter
